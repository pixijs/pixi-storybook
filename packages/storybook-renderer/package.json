{
  "name": "@pixi/storybook-renderer",
  "version": "1.0.0-alpha.0",
  "description": "Storybook PixiJS renderer",
  "homepage": "https://github.com/pixijs/storybook/tree/main/packages/storybook-renderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/storybook.git",
    "directory": "packages/storybook-renderer"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preview": {
      "require": "./dist/config.js",
      "import": "./dist/config.mjs",
      "types": "./dist/config.d.ts"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json",
      "types": "./package.json"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "check": "tsc --emitDeclarationOnly false --noEmit",
    "build": "../../scripts/prepare/bundle.ts && tsc"
  },
  "dependencies": {
    "@storybook/csf": "~0.1.2",
    "@storybook/docs-tools": "^8.0.0",
    "@storybook/manager-api": "^8.0.0",
    "@storybook/preview-api": "^8.0.0",
    "deep-equal": "^2.2.3",
    "global": "^4.4.0",
    "ts-dedent": "^2.2.0"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.4",
    "@types/webpack-env": "^1.18.4",
    "pixi.js": "^8.0.0",
    "typescript": "~5.3.3"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "pixi.js": "^7.0.0 || ^8.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/config.ts"
    ],
    "platform": "browser"
  }
}
